    "terminal.ansiBlue": "#8FC8FF",
    "terminal.ansiBrightBlue": "#8FC8FF",
    "terminal.ansiBrightCyan": "#41BFFF",
    "terminal.ansiBrightGreen": "#85EEA7",
    "terminal.ansiBrightMagenta": "#F806FA",
    "terminal.ansiBrightRed": "#FF407B",
    "terminal.ansiBrightYellow": "#FFEF16",
    "terminal.ansiCyan": "#41BFFF",
    "terminal.ansiGreen": "#85EEA7",
    "terminal.ansiMagenta": "#F806FA",
    "terminal.ansiRed": "#FF407B",
    "terminal.ansiYellow": "#FFEF16",
    "terminal.selectionBackground": "#ffffff3b",
    "terminalCursor.background": "#A8C3C8",
    "terminalCursor.foreground": "#D58FDB",

    "debugToolBar.background": "#1C1E26",

    "walkThrough.embeddedEditorBackground": "#232530",

    "gitDecoration.addedResourceForeground": "#D58FDB",
    "gitDecoration.modifiedResourceForeground": "#FFEF16",
    "gitDecoration.deletedResourceForeground": "#FF407B",
    "gitDecoration.untrackedResourceForeground": "#85EEA7",
    "gitDecoration.ignoredResourceForeground": "#D5D8DA59"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#525863",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation within templates",
      "scope": "string.template meta.embedded.line",
      "settings": {
        "foreground": "##F6F8F7"
      }
    },
    {
      "name": "Parameter & Other",
      "scope": ["variable.parameter", "variable.other", "constant.language"],
      "settings": {
        "foreground": "#B4C1D0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage (declaration or modifier keyword)",
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#D58FDB",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Regex",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#B4C1D0"
      }
    },
    {
      "name": "Text",
      "scope": [
        "meta.jsx.children.js",
        "meta.property-value.css",
        "text.html.derivative"
      ],
      "settings": {
        "foreground": "#F6F8F7"
      }
    },
    {
      "name": "Language constant (boolean, null)",
      "scope": "constant.language",
      "settings": {
        "foreground": "#B4C1D0"
      }
    },
    {
      "name": "Character escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Entity",
      "scope": "entity.name",
      "settings": {
        "foreground": "#A8C3C8"
      }
    },

    {
      "name": "Punctuation definition",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.block.js",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "meta.property-list.css",
        "storage.type.function.arrow.js"
      ],
      "settings": {
        "foreground": "#D58FDB"
      }
    },
    {
      "name": "Tag atttribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#D58FDB",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "variable.function",
        "meta.export.default.js"
      ],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#D167B5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Control keyword (if, try, while, etc.)",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#D58FDB",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#D58FDB",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Unit",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#B4C1D0"
      }
    },
    {
      "name": "Support",
      "scope": "support",
      "settings": {
        "foreground": "#B4C1D0"
      }
    },
    {
      "name": "Support function",
      "scope": "support.function",
      "settings": {
        "foreground": "#F6F8F7"
      }
    },
    {
      "name": "Support variable",
      "scope": "support.variable",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Object literal key / property",
      "scope": ["meta.object-literal.key", "support.type.property-name"],
      "settings": {
        "foreground": "#D58FDB"
      }
    },
    {
      "name": "JS Variable Property",
      "scope": "variable.other.property.js",
      "settings": {
        "foreground": "#B4C1D0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JS Variable Object",
      "scope": "variable.other.object.js",
      "settings": {
        "foreground": "#F6F8F7"
      }
    },
    {
      "name": "Braces",
      "scope": [
        "meta.brace.square",
        "meta.brace.round",
        "punctuation.definition.block"
      ],
      "settings": {
        "foreground": "#A8C3C8"
      }
    },
    {
      "name": "Key-value separator",
      "scope": "punctuation.separator.key-value",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Embedded puncuation",
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Puncuation Definition block",
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Template expression",
      "scope": ["string.template"],
      "settings": {
        "foreground": "#D58FDB"
      }
    },
    {
      "name": "CSS property",
      "scope": [
        "support.type.property-name.css",
        "support.type.vendored.property-name.css",
        "meta.property-name"
      ],
      "settings": {
        "foreground": "#D58FDB"
      }
    },
    {
      "name": "Font names",
      "scope": "support.constant.font-name",
      "settings": {
        "foreground": "#B4C1D0"
      }
    },
    {
      "name": "CSS #id",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#D167B5"
      }
    },
    {
      "name": "Pseudo CSS",
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#F6F8F7"
      }
    },
    {
      "name": "Markup heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#B4C1D0"
      }
    },
    {
      "name": "Markup quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#D167B5"
      }
    },
    {
      "name": "Markup list",
      "scope": "beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#B4C1D0"
      }
    },
    {
      "name": "Markup link",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#F6F8F7"
      }
    },
    {
      "name": "Markup link description",
      "scope": "string.other.link.description",
      "settings": {
        "foreground": "#B4C1D0"
      }
    },
    {
      "name": "Yaml Tag",
      "scope": [
        "entity.name.tag.yaml",
        "punctuation.definition.block.sequence.item.yaml"
      ],
      "settings": {
        "foreground": "#D167B5"
      }
    },
    {
      "name": "Python function call",
      "scope": "meta.function-call.generic.python",
      "settings": {
        "foreground": "#F6F8F7"
      }
    },
    {
      "name": "C# storage type",
      "scope": "storage.type.cs",
      "settings": {
        "foreground": "#D167B5"
      }
    },
    {
      "name": "C# local variable",
      "scope": "entity.name.variable.local.cs",
      "settings": {
        "foreground": "#B4C1D0"
      }
    },
    {
      "name": "C# properties and fields",
      "scope": [
        "entity.name.variable.field.cs",
        "entity.name.variable.property.cs"
      ],
      "settings": {
        "foreground": "#B4C1D0"
      }
    },
    {
      "name": "C++ operators",
      "scope": "source.cpp keyword.operator",
      "settings": {
        "foreground": "#D167B5"
      }
    },
    {
      "name": "Markdown Punctuation heading",
      "scope": "punctuation.definition.heading.markdown",
      "settings": {
        "foreground": "#D167B5"
      }
    },
    {
      "name": "Markdown Punctuation list",
      "scope": "punctuation.definition.list",
      "settings": {
        "foreground": "#D167B5"
      }
    },
    {
      "name": "Markdown Bold Punctuation",
      "scope": "punctuation.definition.bold.markdown",
      "settings": {
        "foreground": "#D58FDB7a"
      }
    },
    {
      "name": "Markdown Italic Punctuation",
      "scope": "punctuation.definition.italic.markdown",
      "settings": {
        "foreground": "#ffffff7e"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold.markdown", "entity.name.section.markdown"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": "markup.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}
